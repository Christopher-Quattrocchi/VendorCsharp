<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <link rel="stylesheet" href="~/css/site.css">
  <title>Vendor Index</title>
  @* i am views/vendors/index *@

</head>

<body>    
  <h1>List of all vendors</h1>
  @foreach (var vendor in Model)
  {
    <p>@vendor.Name</p>
    <a href="/vendors/@vendor.Id">View Details</a>
  }

<a href="/vendors/new">Add a new vendor</a>
<a href="/">Back to splash screen</a>
</body>

</html>